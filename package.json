{
  "name": "testcafe",
  "version": "1.0.0",
  "description": "Automated testing with TestCafe",
  "main": "index.js",
  "scripts": {
    "test:http": "testcafe \"chrome --incognito --disable-http2 --disable-features=StrictOriginIsolation\" ./tests",
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true --skip-js-errors --ssl-insecure --debug-on-fail",
    "test:edge": "testcafe edge ./tests -s takeOnFails=true --skip-js-errors",
    "test:chrome:report": "node reportWithTime.js testcafe chrome ./tests -s takeOnFails=true --skip-js-errors",
    "test:chrome:2": "node reportWithTime.js testcafe -c 2 chrome ./tests -s takeOnFails=true --skip-js-errors",
    "test:chrome:headless": "testcafe chrome:headless ./tests -s takeOnFails=true",
    "test:chrome:mobile": "node reportWithTime.js testcafe chrome:emulation:device=iphone X ./tests -s takeOnFails=true",
    "test:edge:report": "node reportWithTime.js testcafe edge ./tests -s takeOnFails=true",
    "test:edge:5": "node reportWithTime.js testcafe -c 5 edge ./tests -s takeOnFails=true",
    "test:edge:headless": "node reportWithTime.js testcafe edge:headless ./tests -s takeOnFails=true",
    "test:edge:headless:5": "node reportWithTime.js testcafe -c 5 edge:headless ./tests -s takeOnFails=true",
    "test:multiple": "node reportWithTime.js testcafe chrome,edge ./tests -s takeOnFails=true",
    "test": "npm run test:chrome && npm run test:edge"
  },
  "keywords": [
    "TestCafe",
    "Testing",
    "Automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prettier": "^3.5.3",
    "testcafe": "^3.7.2"
  },
  "devDependencies": {
    "testcafe-reporter-html": "^1.4.6"
  }
}
